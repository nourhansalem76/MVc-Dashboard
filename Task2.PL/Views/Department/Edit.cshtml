@model DepartmentViewModel
@{
    ViewData["Title"] = "Edit";
}

<h1 class="mb-4">Edit Department</h1>

<form asp-action="Edit">
    <div asp-validation-summary="All"></div>
    <partial name="PartialViews/CreateEditDepartmentPartialView" />

<input type="submit" value="Save" class="btn btn-primary ml-2" />
<a asp-controller="Department" asp-action="Index" class="btn btn-danger">Back To List</a>

</form>

@section ValidationScriptSection
    {

    <partial name="_ValidationScriptsPartial" />
}




