@model DepartmentViewModel
@{
    ViewData["Title"] = "Delete";
}

<h1 class="mb-4">Are You Sure To Delete This Department!</h1>

<form asp-action="Delete">
    <div asp-validation-summary="All"></div>
    <partial name="PartialViews/DetailsDeleteDepartmentPartialView" />
    <input type="submit" value="Delete" class="btn btn-danger"/>
    <a asp-controller="Department" asp-action="Index" class="btn btn-success">Cancel</a>
</form>



